{
  "name": "@liga/backoffice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve --configuration hmr --hmr",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production --aot",
    "deploy:aws": "node aws-deploy.js",
    "lint": "ng lint",
    "lint:fix": "ng lint backoffice --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@angular/animations": "^13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/router": "^13.2.2",
    "@nebular/auth": "^9.0.1",
    "@nebular/eva-icons": "^9.0.1",
    "@nebular/security": "^9.0.1",
    "@nebular/theme": "^9.0.1",
    "@nestjsx/crud-request": "^4.6.2",
    "bootstrap": "4.3.1",
    "core-js": "2.5.1",
    "eva-icons": "^1.1.3",
    "file-type": "^17.1.1",
    "jwt-decode": "^3.0.0",
    "nebular-icons": "1.1.0",
    "pace-js": "1.0.2",
    "roboto-fontface": "~0.10.0",
    "rxjs": "7.5.4",
    "style-loader": "^1.1.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@types/file-type": "^10.9.1",
    "@types/node": "^14.18.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.12.0",
    "acorn": "^8.7.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "37.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^7.0.4",
    "rimraf": "3.0.2",
    "stylelint": "^14.5.0",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  }
}
