{
  "name": "@liga/backoffice",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve --configuration hmr --hmr",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production --aot",
    "release": "cross-env HUSKY=0 node scripts/release.js",
    "deploy:aws": "node aws-deploy.js",
    "lint": "ng lint",
    "lint:fix": "ng lint backoffice --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@nebular/auth": "9.0.3",
    "@nebular/eva-icons": "9.0.3",
    "@nebular/security": "9.0.3",
    "@nebular/theme": "9.0.3",
    "@nestjsx/crud-request": "4.6.2",
    "bootstrap": "4.6.1",
    "core-js": "2.6.12",
    "eva-icons": "1.1.3",
    "jwt-decode": "3.1.2",
    "nebular-icons": "1.1.0",
    "pace-js": "1.2.4",
    "roboto-fontface": "~0.10.0",
    "rxjs": "7.5.5",
    "style-loader": "1.3.0",
    "tslib": "2.4.0",
    "xlsx": "0.18.5",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.8",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@jsdevtools/ez-spawn": "3.0.4",
    "@jsdevtools/version-bump-prompt": "6.1.0",
    "@types/node": "14.18.21",
    "@types/xlsx": "0.0.36",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.12.0",
    "acorn": "8.7.1",
    "auto-changelog": "2.4.0",
    "capacitor-set-version": "2.0.3",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "7.0.4",
    "rimraf": "3.0.2",
    "stylelint": "14.5.0",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  }
}
